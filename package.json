{
  "name": "@spudly/stephensorensen.com",
  "private": true,
  "version": "0.0.9",
  "devDependencies": {
    "@spudly/eslint-config": "^5.0.1",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.17.1",
    "lint-staged": "^3.4.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.3.0"
  },
  "scripts": {
    "start": "babel-node src/server/index.js",
    "test": "jest",
    "lint:staged": "lint-staged",
    "lint": "eslint ."
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 7.5
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@spudly"
  },
  "now": {
    "alias": "stephensorensen.com"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write --print-width=100 --single-quote --bracket-spacing=false --trailing-comma=all",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "http-fantasy-land": "^2.1.0",
    "ramda": "^0.23.0"
  }
}
