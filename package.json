{
  "name": "@spudly/stephensorensen.com",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build:clean": "rm -rf build",
    "build:js:server-side": "tsc",
    "build:js:client-side": "webpack",
    "build:html": "cp -R src/html build",
    "build:css": "postcss -o build/css/index.css src/css/index.css",
    "build": "npm run build:clean; npm run build:js:server-side && npm run build:js:client-side && npm run build:html && npm run build:css",
    "lint": "eslint .",
    "prepare": "npm run build",
    "start": "nodemon -L",
    "deploy": "now",
    "now-start": "node ./build/js/server-side/index.js",
    "deploy:alias": "now alias"
  },
  "hooks": {
    "pre-commit": "pretty-quick --staged"
  },
  "engines": {
    "node": ">=8.x.x"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.10.5",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/webpack-dev-middleware": "^2.0.2",
    "babel-eslint": "9.x",
    "cssnano": "^4.1.10",
    "eslint": "5.x",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "husky": "^1.3.1",
    "nodemon": "^1.18.10",
    "now": "^14.0.3",
    "postcss-cli": "^6.1.2",
    "postcss-middleware": "^1.1.4",
    "postcss-preset-env": "^6.6.0",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.1"
  },
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "license": "UNLICENSED"
}
