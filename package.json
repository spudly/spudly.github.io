{
  "name": "@spudly/stephensorensen.com",
  "private": true,
  "version": "0.0.9",
  "devDependencies": {
    "@spudly/eslint-config": "^5.1.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.7.2",
    "lint-staged": "^4.2.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.7.0"
  },
  "scripts": {
    "start": "node src/server/index.js",
    "test": "jest",
    "lint:staged": "lint-staged",
    "lint": "eslint ."
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 8.5
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@spudly",
    "parserOptions": {
      "sourceType": "script"
    }
  },
  "now": {
    "alias": "stephensorensen.com"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write --print-width=100 --single-quote --bracket-spacing=false --trailing-comma=all",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "http-fantasy-land": "^3.0.0",
    "ramda": "^0.24.1"
  },
  "engines": {
    "node": "8.5.x"
  }
}
