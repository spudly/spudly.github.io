{
  "name": "@spudly/stephensorensen.com",
  "private": true,
  "version": "0.0.9",
  "devDependencies": {
    "lint-staged": "^7.0.4",
    "now": "^11.1.7",
    "prettier": "^1.12.1",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "now-start": "serve --single -l 80 dist",
    "build": "rm -rf dist && parcel build src/index.html --public-url .",
    "start": "parcel src/index.html",
    "deploy": "now",
    "deploy:alias": "now alias",
    "lint": "eslint src",
    "ts": "tsc --noEmit --pretty"
  },
  "now": {
    "alias": "stephensorensen.com"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write --print-width=100 --single-quote --bracket-spacing=false --trailing-comma=all",
      "git add"
    ]
  },
  "engines": {
    "node": "9.x.x"
  },
  "dependencies": {
    "parcel-bundler": "^1.7.1",
    "postcss-cssnext": "^3.0.2",
    "serve": "^9.4.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
